void main() {
  problem();
}

void problem() {
  const N = scan!int;
  auto A = scan!long(N);

  void solve() {
    long divideCount2(long a) {
      long count;
      while(a % 2 == 0) {
        count++;
        a /= 2;
      }
      return count;
    }

    A.map!divideCount2.reduce!min.writeln;
  }

  solve();
}

