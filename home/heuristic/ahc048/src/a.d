void problem() {
  auto StartTime = MonoTime.currTime();
  bool elapsed(int ms) { return (ms <= (MonoTime.currTime() - StartTime).total!"msecs"); }
  auto seed = 983_741_243;
  auto RND = Xorshift(seed);
  enum long INF = long.max / 3;

  int N = scan!int;
  int K = scan!int;
  int H = scan!int;
  int T = scan!int;
  int D = scan!int;
  real[][] OWN = scan!real(3 * K).chunks(3).array;
  real[][] TARGET = scan!real(3 * H).chunks(3).array;

  foreach(_; 0..N) {
    writefln("%(%s %)", 1.repeat(N - 1));
  }
  foreach(_; 1..N) {
    writefln("%(%s %)", 1.repeat(N));
  }

  foreach(target; TARGET) {
    real bestScore = int.max;
    int bestOwn;
    foreach(i, own; OWN.enumerate(0)) {
      auto score = 3.iota.map!(i => pow(target[i] - own[i], 2)).sum;
      if (bestScore.chmin(score)) {
        bestOwn = i;
      }
    }

    writefln("1 %s %s %s", 0, 0, bestOwn);
    writefln("2 %s %s", 0, 0);
  }
}

