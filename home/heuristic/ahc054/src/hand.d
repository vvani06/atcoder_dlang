import std;

void main() {
  
  auto grid = [
    "....T....xS.x....T.T",
    "........T.x..x.....T",
    ".......TT..T..x.....",
    ".....T....T.x...xT..",
    ".............x.x....",
    "..............T.....",
    "...............xx...",
    "...TT...........x...",
    "..............T..x..",
    "....T....T..T...T...",
    "...................T",
    ".......T............",
    "T...................",
    "........T.T.....TT..",
    "....................",
    "T..T....T...........",
    "....................",
    "..........TG..T.....",
    "T...........T.......",
    ".........T..........",
