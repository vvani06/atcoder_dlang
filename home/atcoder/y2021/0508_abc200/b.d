void main() { runSolver(); }

void problem() {
  auto N = scan!long;
  auto K = scan!long;

  auto solve() {
    long ans = N;
    foreach(_; 0..K) {
      if (ans % 200 == 0) {
        ans /= 200;
      } else {
        ans *= 1000;
        ans += 200;
      }
    }

    return ans;
  }

  outputForAtCoder(&solve);
}

