void main() { runSolver(); }

void problem() {
  auto N = scan!long;
  auto A = scan!long(N);

  auto solve() {
    foreach(ref a; A) a %= 200;
    const subSize = min(8, N);

    auto routes = new long[][](200, 0);
    foreach(long i; 1..subSize + 1) {
      foreach(il; iota(1, subSize + 1).array.combinations(i)) {
        const mod = il.map!(i => A[i - 1]).sum % 200;
        if (routes[mod].empty) {
          routes[mod] = il.dup;
        } else {
          return [
            YESNO[true],
            (routes[mod].length ~ routes[mod]).toAnswerString,
            (il.length ~ il).toAnswerString,
          ];
        }
      }
    }
    
    return [YESNO[false]];
  }

  outputForAtCoder(&solve);
}

