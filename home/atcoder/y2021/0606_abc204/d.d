void main() { runSolver(); }

void problem() {
  auto N = scan!long;
  auto T = scan!long(N);

  auto solve() {
    long ans = long.max;
    const total = T.sum;

    long[long][] dp;
    dp.length = N + 1;
    dp[0][0] = 1;
    foreach(i, t; T) {
      foreach(x; dp[i].keys) {
        dp[i + 1][x] = 1;
        dp[i + 1][x + t] = 1;

        ans = ans.min(max(x, total - x));
        ans = ans.min(max(x + t, total - x - t));
      }
    }

    return ans;
  }

  outputForAtCoder(&solve);
}

