void main() { runSolver(); }

void problem() {
  auto K = scan!long;
  auto N = scan!long;
  auto M = scan!long;
  auto A = scan!long(K);

  auto solve() {
    auto bases = A.map!(a => a * M).array;
    const ratio = M.to!real / N.to!real;

    alias Rank = Tuple!(long, "index", long, "count");
    auto ranks = A.map!(a => (ratio * a.to!real).to!long)
                  .enumerate
                  .map!(t => Rank(t.index, t.value)).array;

    real score(Rank r) {
      return abs(r.count * N - bases[r.index]);
    }

    long rem = M - ranks.map!(e => e.count).sum;
    foreach(ref rank; ranks.sort!((a, b) => score(a) > score(b))[0..rem]) {
      rank.count++;
    }

    auto ans = new long[](K);
    foreach(h; ranks) ans[h.index] = h.count;
    return ans.toAnswerString;
  }

  outputForAtCoder(&solve);
}

