import std.algorithm.iteration;
void main() { runSolver(); }

void problem() {
  auto N = scan!int;
  auto NQ = scan!int;

  auto solve() {
    alias Coord = Tuple!(int, "x", int, "y");

    auto coords = iota(1, N + 1).retro.map!(x => Coord(x, 0)).array;
    foreach(_; 0..NQ) {
      auto type = scan!int;
      
      if (type == 1) {
        auto dir = scan!char;
        
        auto c = coords[$ - 1];
        if (dir == 'R') c.x++;
        if (dir == 'L') c.x--;
        if (dir == 'U') c.y++;
        if (dir == 'D') c.y--;
        coords ~= c;
      } else {
        auto target = scan!int;

        auto c = coords[$ - target];
        writefln("%d %d", c[0], c[1]);
      }
    }
  }

  outputForAtCoder(&solve);
}

