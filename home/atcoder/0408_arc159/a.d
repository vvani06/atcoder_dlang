void main() { runSolver(); }

void problem() {
  auto N = scan!int;
  auto K = scan!long;
  auto A = scan!int(N^^2).chunks(N).array;
  auto QN = scan!long;
  auto Q = scan!long(2 * QN).chunks(2);

  auto solve() {
    auto distances = new int[][](N, N);
    foreach(ref d; distances) d[] = int.max;
    foreach(from; 0..N) {
      int step = 1;
      auto queue = new DList!int(from);
      while(!queue.empty) {
        auto nexts = new DList!int();
        foreach(cur; queue.array) {
          foreach(next; 0..N) {
            if (cur == next || A[cur][next] == 0 || distances[from][next] != int.max) continue;

            distances[from][next] = step;
            nexts.insertBack(next);
          }
        }

        step++;
        queue = nexts;
      }
    }
    // distances.deb;

    foreach(q; Q) {
      const from = q[0] - 1;
      const fromMod = from % N;
      const to = q[1] - 1;
      const toMod = to % N;

      if (fromMod == toMod && A[fromMod][toMod] == 1) {
        1.writeln;
      } else {
        const ans = distances[fromMod][toMod];
        (ans == int.max ? -1 : ans).writeln;
      }
    }
  }

  outputForAtCoder(&solve);
}

