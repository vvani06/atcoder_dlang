void main() { runSolver(); }

void problem() {
  auto N = scan!long;
  auto A = scan!long(N);
  auto B = scan!long(N);
  auto QN = scan!long;
  auto Q = scan!long(2 * QN).chunks(2);
  enum long MOD = 998_244_353;

  auto solve() {
    long[] a = new long[](N + 1);
    long[] b = new long[](N + 1);
    a[0] = b[0] = MOD - 1;
    bool[long] va, vb;

    foreach(i; 0..N) {
      a[i + 1] = a[i];
      b[i + 1] = b[i];
      if (!(A[i] in va)) {
        a[i + 1] = (a[i] * A[i]) % MOD;
        va[A[i]] = true;
      }
      if (!(B[i] in vb)) {
        b[i + 1] = (b[i] * B[i]) % MOD;
        vb[B[i]] = true;
      }
    }
    
    foreach(q; Q) {
      const x = q[0];
      const y = q[1];

      YESNO[a[x] == b[y]].writeln;
    }
  }

  outputForAtCoder(&solve);
}

