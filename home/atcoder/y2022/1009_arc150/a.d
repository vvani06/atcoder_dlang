void main() { runSolver(); }

void problem() {
  alias Case = Tuple!(int, "N", int, "K", string, "S");
  auto T = scan!int;
  auto Cases = T.iota.map!(_ => Case(scan!int, scan!int, scan)).array;

  auto solve() {
    bool judge(string s, long k) {
      long allOnes = s.count('1');
      if (allOnes > k) return false;

      long ones = s[0..k - 1].count('1');
      long qs = s[0..k - 1].count('?');
      int ans;
      bool pre1, suf1;
      foreach(i; 0..s.length - k + 1) {
        if (s[k + i - 1] == '1') ones++;
        if (s[k + i - 1] == '?') qs++;

        suf1 = i < s.length - k && s[k + i] == '1';
        // [i, ones, qs].deb;

        if (!pre1 && !suf1 && ones + qs == k && allOnes + qs == k) {
          ans++;
        }

        if (s[i] == '1') ones--;
        if (s[i] == '?') qs--;
        pre1 = s[i] == '1';
      }

      return ans == 1;
    }

    foreach(c; Cases) {
      YESNO[judge(c.S, c.K)].writeln;
    }
  }

  outputForAtCoder(&solve);
}

