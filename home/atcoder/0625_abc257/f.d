void main() { runSolver(); }

void problem() {
  auto N = scan!int;
  auto M = scan!int;
  auto E = scan!int(2 * M).chunks(2);

  auto solve() {
    auto graph = new int[][](N + 1, 0);
    foreach(e; E) {
      graph[e[0]] ~= e[1];
      graph[e[1]] ~= e[0];
    }

    long[] calcDistances(int start) {
      auto ret = new long[](N + 1);
      ret[] = int.max;
      ret[start] = 0;
      for(auto q = DList!int(start); !q.empty;) {
        auto p = q.front; q.removeFront;
        foreach(to; graph[p]) {
          if (ret[to] != int.max) continue;

          if (to == 0 || p == 0) {
            ret[to] = ret[p];
            q.insertFront(to);
          } else {
            ret[to] = ret[p] + 1;
            q.insertBack(to);
          }
        }
      }

      return ret;
    }

    auto from1 = calcDistances(1);
    auto fromN = calcDistances(N);
    [from1, fromN].each!deb;

    auto zero = graph[0].dup.sort;
    
    long[] ans = min(from1[N], from1[0] + fromN[0] + 1).repeat(N).array;
    if (fromN[0] != int.max) ans[0] = min(ans[0], fromN[0] + 1);
    if (from1[0] != int.max) ans[N - 1] = min(ans[N - 1], from1[0] + 1);
    return ans.map!(a => a >= int.max ? -1 : a).toAnswerString;
  }

  outputForAtCoder(&solve);
}

